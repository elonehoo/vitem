{
  "name": "@vitem/monorepo",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "example:dev":"npm -C example run dev",
    "example:build":"npm -C example run build",
    "example:preview":"npm -C example run preview"
  },
  "devDependencies": {
    "@vitem/footer":"workspace:*",
    "@vitem/error":"workspace:*",
    "@vitem/main":"workspace:*",
    "@vitem/hi":"workspace:*",
    "@elonehoo/eslint-config": "^2.0.0",
    "@iconify-json/carbon": "^1.1.4",
    "@types/node": "^17.0.31",
    "@unocss/reset": "^0.32.9",
    "@vitejs/plugin-vue": "^2.3.1",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "pnpm": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "unbuild": "^0.7.4",
    "unocss": "^0.32.9",
    "vite": "^2.9.5",
    "vitest": "^0.10.1",
    "vue": "^3.2.25",
    "vue-router": "4",
    "vue-tsc": "^0.34.7"
  }
}
