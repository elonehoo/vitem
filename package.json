{
  "name": "@vitem/monorepo",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "example:dev": "npm -C example run dev",
    "example:build": "npm -C example run build",
    "example:preview": "npm -C example run preview"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "^2.0.0",
    "@iconify-json/carbon": "^1.1.6",
    "@types/node": "^18.0.0",
    "@unocss/reset": "^0.41.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitem/error": "workspace:*",
    "@vitem/footer": "workspace:*",
    "@vitem/hi": "workspace:*",
    "@vitem/main": "workspace:*",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "pnpm": "^7.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "unocss": "^0.41.1",
    "vite": "^2.9.12",
    "vitest": "^0.16.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.38.2"
  }
}
