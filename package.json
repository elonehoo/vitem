{
  "name": "@vitem/monorepo",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "example:dev": "npm -C example run dev",
    "example:build": "npm -C example run build",
    "example:preview": "npm -C example run preview"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@iconify-json/carbon": "1.1.7",
    "@types/node": "18.7.13",
    "@unocss/reset": "0.45.12",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitem/error": "workspace:*",
    "@vitem/footer": "workspace:*",
    "@vitem/hi": "workspace:*",
    "@vitem/main": "workspace:*",
    "bumpp": "8.2.1",
    "eslint": "8.22.0",
    "pnpm": "7.9.5",
    "rimraf": "3.0.2",
    "typescript": "4.7.4",
    "unbuild": "0.8.9",
    "unocss": "0.44.7",
    "vite": "3.0.9",
    "vitest": "0.18.1",
    "vue": "3.2.37",
    "vue-router": "4.1.4",
    "vue-tsc": "0.38.9"
  }
}
